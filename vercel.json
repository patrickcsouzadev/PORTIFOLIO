{
  "version": 2,
  "buildCommand": "echo 'Build complete'",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/index.py"
    },
    {
      "source": "/api",
      "destination": "/api/index.py"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.py"
    },
    {
      "src": "/api",
      "dest": "/api/index.py"
    },
    {
      "handle": "filesystem"
    }
  ]
}
